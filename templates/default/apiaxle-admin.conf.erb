server {
    listen 80 default_server;
    listen [::]:80 default_server ipv6only=on;
    root /usr/local/src/apiaxle-admin/releases/v1;
    index index.html index.htm;
    server_name localhost;
    allow 10.0.0.0/8;
    deny all;
    location /v1 {
        proxy_pass http://localhost:3000;
    }
}
