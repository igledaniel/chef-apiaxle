{
  "redis": {
    "host": "<%= @redis_host %>",
    "port": <%= @redis_port %>
  },
  "application": {
    "debug": true
  },
  "logging": {
    "level": "<%= @log_level %>",
    "appenders": [{
      "type": "console"
    }]
  },
  "hit_processors": <%= node[:apiaxle][:hit_processors].map do |p|
    {
      path: p['require_path'],
      args: p['args']
    }
  end.to_json %>
}
